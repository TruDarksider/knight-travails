{
  "compilerOptions": {
    "target": "es5",                                  
    "module": "es6",                                
    "allowJs": true,                                   
    "outDir": "./dist/",                                   
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    //"skipLibCheck": true
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true                                
  },
  //"include": ["./src/**/*"]
}
